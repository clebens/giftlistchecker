<h1>Editing giftlist</h1>

<% form_for(@giftlist) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :giftlist_name %><br />
    <%= f.text_field :giftlist_name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @giftlist %> |
<%= link_to 'Back', giftlists_path %>